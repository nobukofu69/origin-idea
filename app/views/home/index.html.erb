<h2>コンサルタント一覧</h2>
<% @users.each do |user| %>
  <%# コンサルタントのみ表示 %>
  <% if user.is_consultant %>
    <div class="card">
      <div class="card-body">
        <%= link_to user_path(user) do %>
          <h5 class="card-title"><%= user.username %></h5>
          <p class="card-text"><%= user.profession %></p>
          <p class="card-text"><%= user.rating %></p>
        <% end %>
      </div>
  <% end %>
<% end %>